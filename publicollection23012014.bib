	Institution = {Institut Pasteur, Microbial Evolutionary Genomics, CNRS, URA2171, F-75015 Paris, France.},
	Journal = {Microbiology and Molecular Biology Reviews},
	Keywords = {bacterial,bacterial genetics,dna,genomics,genomics methods,phylogeny,plasmids,plasmids genetics},
	Number = {3},
	Pages = {434--452},
	Publisher = {American Society for Microbiology (ASM)},
	Title = {{Mobility of plasmids}},
