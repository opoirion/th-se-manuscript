	Journal = {Biochimica et Biophysica Acta - Gene Regulatory Mechanisms},
	Keywords = {Communication between chromosome replication and s,Divided genome,Genome evolution,Genome maintenance,Secondary chromosomes,V. cholerae},
	Number = {7},
	Pages = {826--829},
