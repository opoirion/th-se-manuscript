	Journal = {BMC bioinformatics},
	Month = nov,
	Number = {1},
	Pages = {307},
	Pmid = {23157331},
	Title = {{Multiple graph regularized protein domain ranking.}},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/23157331},
	Volume = {13},
	Year = {2012},
	Bdsk-Url-1 = {http://www.ncbi.nlm.nih.gov/pubmed/23157331},
