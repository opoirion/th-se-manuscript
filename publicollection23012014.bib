	Journal = {Genome biology and evolution},
	Keywords = {Evolution, Molecular,Genome, Bacterial,Genome, Bacterial: genetics,Microscopy, Electron, Transmission,Molecular Sequence Data,Operon,Symbiosis,Symbiosis: genetics,Tryptophan,Tryptophan: metabolism},
	Month = jan,
	Pages = {708--18},
	Pmid = {20829280},
	Title = {{Functional convergence in reduced genomes of bacterial symbionts spanning 200 My of evolution.}},
	Url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2953269\&tool=pmcentrez\&rendertype=abstract},
	Volume = {2},
	Year = {2010},
	Bdsk-Url-1 = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2953269%5C&tool=pmcentrez%5C&rendertype=abstract},
	Bdsk-Url-2 = {http://dx.doi.org/10.1093/gbe/evq055}}

@article{Menard2013,
	Abstract = {Integrative and conjugative elements (ICEs) are widespread mobile genetic elements that are usually found integrated in bacterial chromosomes. They are important agents of evolution and contribute to the acquisition of new traits, including antibiotic resistances. ICEs can excise from the chromosome and transfer to recipients by conjugation. Many ICEs are site-specific in that they integrate preferentially into a primary attachment site in the bacterial genome. Site-specific ICEs can also integrate into secondary locations, particularly if the primary site is absent. However, little is known about the consequences of integration of ICEs into alternative attachment sites or what drives the apparent maintenance and prevalence of the many ICEs that use a single attachment site. Using ICEBs1, a site-specific ICE from Bacillus subtilis that integrates into a tRNA gene, we found that integration into secondary sites was detrimental to both ICEBs1 and the host cell. Excision of ICEBs1 from secondary 
sites was impaired either partially or completely, limiting the spread of ICEBs1. Furthermore, induction of ICEBs1 gene expression caused a substantial drop in proliferation and cell viability within three hours. This drop was dependent on rolling circle replication of ICEBs1 that was unable to excise from the chromosome. Together, these detrimental effects provide selective pressure against the survival and dissemination of ICEs that have integrated into alternative sites and may explain the maintenance of site-specific integration for many ICEs.},
	Author = {Menard, Kayla L and Grossman, Alan D},
	Doi = {10.1371/journal.pgen.1003623},
	File = {:home/oliver/Documents/publi/publi bio/recombinase/ICE\_attachment\_site\_specificity\_maintenance\_selective\_pressures.pdf:pdf},
	Issn = {1553-7404},
	Journal = {PLoS genetics},
	Month = jul,
	Number = {7},
	Pages = {e1003623},
	Pmid = {23874222},
	Title = {{Selective pressures to maintain attachment site specificity of integrative and conjugative elements.}},
	Url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3715440\&tool=pmcentrez\&rendertype=abstract},
	Volume = {9},
	Year = {2013},
	Bdsk-Url-1 = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3715440%5C&tool=pmcentrez%5C&rendertype=abstract},
	Bdsk-Url-2 = {http://dx.doi.org/10.1371/journal.pgen.1003623}}

