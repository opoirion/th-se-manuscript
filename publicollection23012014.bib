	Month = oct,
	Number = {2},
	Pages = {278--89},
	Pmid = {20735775},
	Title = {{Cell division in a minimal bacterium in the absence of ftsZ.}},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/20735775},
