	Keywords = {Algorithms,Computational Biology,Evolution, Molecular,Gene Transfer, Horizontal,Gene Transfer, Horizontal: genetics,Genome,Genome, Archaeal,Genome, Bacterial,Genome, Fungal,Models, Genetic},
	Month = oct,
	Number = {10},
	Pages = {e316},
	Pmid = {16122348},
	Title = {{The cobweb of life revealed by genome-scale estimates of horizontal gene transfer.}},
	Url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1233574\&tool=pmcentrez\&rendertype=abstract},
	Volume = {3},
