	Issn = {1432-0614},
	Journal = {Applied microbiology and biotechnology},
	Keywords = {chromid,compatibility,concept,conjugation,plasmid classification,plasmid curing,rhodobacterales},
	Month = feb,
	Pages = {2805--2815},
	Pmid = {23435940},
	Title = {{Extrachromosomal, extraordinary and essential-the plasmids of the Roseobacter clade.}},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/23435940},
	Year = {2013},
