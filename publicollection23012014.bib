	Journal = {BMC bioinformatics},
	Keywords = {Algorithms,Classification,Classification: methods,Cluster Analysis,Computational Biology,Computational Biology: methods,DNA,DNA: analysis,Risk Factors},
	Month = jan,
	Pages = {S4},
	Pmid = {17118147},
	Title = {{Support vector machine implementations for classification \& clustering.}},
