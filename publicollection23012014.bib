	Keywords = {Adipates,Adipates: metabolism,Brucella,Brucella: classification,Brucella: genetics,Brucella: physiology,Chromosomes, Bacterial,Chromosomes, Bacterial: genetics,Computational Biology,Gene Transfer, Horizontal,Gene Transfer, Horizontal: genetics,Genome, Bacterial,Genome, Bacterial: genetics,Models, Genetic,Phylogeny,Pseudogenes,Pseudogenes: genetics,Signal Transduction,Signal Transduction: genetics},
	Number = {11},
