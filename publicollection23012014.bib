	Issn = {1092-2172},
	Journal = {Microbiology and Molecular Biology Reviews},
	Keywords = {Base Sequence,DNA Replication,DNA, Bacterial,Leucine Zippers,Leucine Zippers: genetics,Models, Genetic,Molecular Sequence Data,Plasmids,Plasmids: genetics,Plasmids: physiology,RNA, Antisense,Sequence Alignment},
	Number = {2},
	Pages = {434--64},
	Pmid = {9618448},
	Title = {{Replication and control of circular bacterial plasmids}},
	Url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=98921\&tool=pmcentrez\&rendertype=abstract},
	Volume = {62},
