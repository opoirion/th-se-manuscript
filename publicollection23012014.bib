@article{Lederberg,
	Abstract = {The term "plasmid" was introduced 45 years ago (J. Lederberg, 1952, Physiol. Rev. 32, 403- 430) as a generic term for any extrachromosomal genetic particle. It was intended to clarify the classification of agents that had been thought of disjunctively as parasites, symbionts, organelles, or genes. For a decade or more it was confused with "episome," although that was carefully crafted (F. Jacob and E. L. Wollman, 1958, C. R. Acad. Sci. 247, 154-156) to mean agents with traffic in and out of chromosomes. Starting about 1970, plasmids became important reagents in molecular genetic research and biotechnology. They also play a cardinal role in the evolution of microbial resistance and of pathogenicity. The usage of the term has then escalated to its current peak of about 3000 published articles per year. The bedrock of genetic mechanism is no longer mitosis and meiosis of chromosomes; it is template-directed DNA assembly. This is often more readily studied and managed with the use of plasmids, which 
replicate autonomously outside the chromosomes. Some plasmids are also episomes, namely, they interact with the chromosomal genome, and other mobile elements may be transposed from one chromosomal locus to another without replicating autonomously. (C) 1998 Academic Press.},
	Author = {Lederberg},
	Title = {{ Plasmid (1952-1997)}},
	Url = {http://apps.webofknowledge.com/full\_record.do?product=UA\&search\_mode=GeneralSearch\&qid=9\&SID=Y27GhoBJ7kga6LPmg4P\&page=1\&doc=1},
	Bdsk-Url-1 = {http://apps.webofknowledge.com/full%5C_record.do?product=UA%5C&search%5C_mode=GeneralSearch%5C&qid=9%5C&SID=Y27GhoBJ7kga6LPmg4P%5C&page=1%5C&doc=1}}

