	Keywords = {Base Sequence,Chromosomes, Bacterial,Cyanothece,Cyanothece: cytology,Cyanothece: genetics,Cyanothece: metabolism,Energy Metabolism,Energy Metabolism: genetics,Fermentation,Fermentation: genetics,Gene Order,Genome, Bacterial,Molecular Sequence Data,Nitrogen Fixation,Nitrogen Fixation: genetics,Proteomics,Pyruvic Acid,Pyruvic Acid: metabolism,Sequence Analysis, DNA},
