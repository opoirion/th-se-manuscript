	Journal = {FEMS Microbiology Reviews},
	Keywords = {Adaptation, Biological,Bacteria,Bacteria: genetics,Biological Evolution,Gene Transfer, Horizontal,Genetic Variation,Plasmids,Recombination, Genetic,Selection, Genetic},
	Number = {5},
	Pages = {957--76},
	Pmid = {21711367},
