	Keywords = {Animals,Biological Evolution,Gene Transfer, Horizontal,Genetic Variation,Humans,Models, Genetic,Phylogeny},
	Number = {7},
	Pages = {2043--9},
	Pmid = {17261804},
	Title = {{Pattern pluralism and the Tree of Life hypothesis.}},
	Url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1892968\&tool=pmcentrez\&rendertype=abstract},
	Volume = {104},
