	Issn = {1367-4811},
	Journal = {Bioinformatics (Oxford, England)},
	Month = jun,
	Number = {11},
	Pages = {1424--32},
	Pmid = {23572412},
	Title = {{Measuring gene functional similarity based on group-wise comparison of GO terms.}},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/23572412},
	Volume = {29},
	Year = {2013},
