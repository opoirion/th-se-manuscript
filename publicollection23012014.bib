	Issn = {1553-7358},
	Journal = {PLoS computational biology},
	Keywords = {Binding Sites,DNA,DNA-Binding Proteins,DNA-Binding Proteins: metabolism,DNA: metabolism,Escherichia coli Proteins,Escherichia coli Proteins: metabolism,Models, Molecular,Protein Binding},
	Month = jan,
	Number = {1},
	Pages = {e1002881},
