	Keywords = {Bacteria,Bacteria: classification,Bacteria: genetics,Biodiversity,Biological Evolution,Ecosystem,Extinction, Biological,Genes, Bacterial,Genetic Speciation,Genetic Variation,Models, Biological,Models, Genetic,Recombination, Genetic,Selection, Genetic},
