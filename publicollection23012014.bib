	Journal = {Journal of Bacteriology},
	Keywords = {Bacteria,Bacteria: genetics,Base Sequence,Centromere,Centromere: genetics,Chromosomes, Bacterial,Chromosomes, Bacterial: genetics,Computational Biology,Genome, Bacterial,Genomics,Genomics: methods,Phylogeny},
	Number = {23},
