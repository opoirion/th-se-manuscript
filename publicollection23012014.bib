@article{Song2007,
	Abstract = {Homology identification is the first step for many genomic studies. Current methods, based on sequence comparison, can result in a substantial number of mis-assignments due to the similarity of homologous domains in otherwise unrelated sequences. Here we propose methods to detect homologs through explicit comparison of protein domain content. We developed several schemes for scoring the homology of a pair of protein sequences based on methods used in the field of information retrieval. We evaluate the proposed methods and methods used in the literature using a benchmark of fifteen sequence families of known evolutionary history. The results of these studies demonstrate the effectiveness of comparing domain architectures using these similarity measures. We also demonstrate the importance of both weighting promiscuous domains and of compensating for the statistical effect of having a large number of domains in a protein. Using logistic regression, we demonstrate the benefit of combining similarity 
measures based on domain content with sequence similarity measures.},
	Author = {Song, N and Sedgewick, R D and Durand, D},
	Date-Modified = {2015-03-20 15:34:14 +0000},
	Doi = {10.1089/cmb.2007.A009},
	File = {:home/oliver/Documents/publi/publi bioinfo/functionnal homologs clustering/Domain Architecture Comparison for Multidomain Homology Identification.pdf:pdf},
	Issn = {1066-5277},
	Journal = {Journal of Computational Biology},
	Keywords = {Models, Genetic,Protein Structure, Tertiary,Protein Structure, Tertiary: genetics,Proteins,Proteins: genetics,Sequence Analysis, Protein,Sequence Homology, Amino Acid},
	Number = {4},
	Pages = {496--516},
	Pmid = {17572026},
	Title = {{Domain architecture comparison for multidomain homology identification}},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/17572026},
	Volume = {14},
	Year = {2007},
