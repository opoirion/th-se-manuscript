	Journal = {PLoS Computational Biology},
	Keywords = {bacteria,cell cycle,chromosome,chromosomes,content,evolution,expression,gene,gene dosage,genes,genome,genomes,hypothesis,multiple,orthologous gene,orthologous genes,pattern,patterns,plasmid,plasmids,prediction,rates,risk,secondary,segregation,selection,selective constraint,theory,translation},
