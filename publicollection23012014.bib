	Journal = {FEMS Microbiology Reviews},
	Keywords = {antibiotic resistance,plasmid,plasmid classification,plasmid conjugation,plasmid replication,population genetics,relaxase},
	Number = {5},
	Pages = {936--956},
	Pmid = {21711366},
	Publisher = {Blackwell Publishing Ltd},
	Title = {{Identification of bacterial plasmids based on mobility and plasmid population biology.}},
