	Journal = {Proceedings of the National Academy of Sciences of the U.S.A.},
	Keywords = {bacterial,bacterial genetics,essential,essential genetics,genes,genome,molecular sequence data,mutation,mutation genetics,mycoplasma genitalium,mycoplasma genitalium genetics},
	Number = {2},
